<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>org.andymatuschak.bear-link-janitor</string>
        <key>WorkingDirectory</key>
        <string>/usr/local/share/bear-link-janitor</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/node</string>
            <string>/usr/local/share/bear-link-janitor/index.js</string>
        </array>
        <key>StartInterval</key>
        <integer>60</integer>
        <key>StandardErrorPath</key>
        <string>/tmp/org.andymatuschak.bear-link-janitor.status.stderr.log</string>
        <key>StandardOutPath</key>
        <string>/tmp/org.andymatuschak.bear-link-janitor.status.stdout.log</string>
    </dict>
